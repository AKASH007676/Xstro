{
   "env": {
      "browser": false,
      "node": true,
      "es2021": true
   },
   "extends": ["eslint:recommended", "plugin:prettier/recommended"],
   "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
   },
   "rules": {
      "no-console": "warn",
      "eqeqeq": "error",
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
   }
}
